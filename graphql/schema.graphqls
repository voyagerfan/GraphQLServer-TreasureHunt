#schema.graphqls

scalar Double

type Greeting {
    title: String
    subtitle: String
}

type Coordinate {
    latitude: Double!
    longitude: Double!
}

type CompletedQuestion {
    questionDetail: String!
}

type QuestItem {
    title: String!
    description: String!
    clue: String!
    hint: String!
    coordinates: Coordinate!
    rating: Double!
    endGameAssets: CompletedQuestion!
}

type Query {
    greetings: [Greeting!]!
    questItems: [QuestItem!]!
}